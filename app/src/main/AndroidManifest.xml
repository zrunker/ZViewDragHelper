<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cc.ibooker.zviewdraghelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BasicViewDragHelperActivity" />
        <activity android:name=".TestBasicViewHelperActivity" />
        <activity android:name=".DelViewDragHelperActivity" />
        <activity android:name="ViewDragHelperLayoutActivity" />
        <activity
            android:name=".SwipeBackLayoutActivity"
            android:theme="@style/TransparentTheme" />
    </application>

</manifest>